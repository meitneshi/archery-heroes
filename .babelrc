{
  "presets": [
    ["es2015", {"modules": false}],

    "react",

    "es2016",
    "es2017"
  ],

  "plugins": [
    ["transform-object-rest-spread", { "useBuiltIns": true }],
    ["transform-class-properties"]
  ],

  "env": {
    "prod": {
      "plugins": [
        "transform-react-remove-prop-types"
      ]
    },
    "dev": {
      "plugins": [
        "react-hot-loader/babel"
      ]
    }
  }
}
